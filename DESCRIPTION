    edgeR (>= 3.9.11),
    GenomeInfoDb (>= 1.3.12),
    genomeIntervals (>= 1.23.0),
    GenomicAlignments (>= 1.3.27),
    GenomicRanges (>= 1.19.36),
    graphics,
    IRanges (>= 2.1.38),
